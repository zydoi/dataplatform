<?xml version="1.0"?>
<web-app>
	<servlet>
		<servlet-name>DataPlatform</servlet-name>
		<servlet-class>
			org.glassfish.jersey.servlet.ServletContainer
		</servlet-class>
		<init-param>
			<param-name>
				javax.ws.rs.Application
			</param-name>
			<param-value>
				com.bimoku.dataplatform.service.DataPlatformApplication
			</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>DataPlatform</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
</web-app>
